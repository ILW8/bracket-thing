{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "",
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "strict": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ]
  }
}
